{
    "name": "libre-informatique/media-bundle",
    "type": "symfony-bundle",
    "description": "Media Bundle for Symfony",
    "require": {
        "php": "^7.1",
        "symfony/symfony": "3.3.*",
        "symfony/event-dispatcher": ">=3.2",
        "data-uri/twig-extension": "^1.0",
        "blast-project/core-bundle": "self.version"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "3.3.*",
        "phpunit/phpunit": "^6.4"
    },
    "license": "LGPL-3.0-only",
    "keywords": [
        "e-venement",
        "libio",
        "sonata",
        "symfony",
        "media"
    ],
    "homepage": "https://github.com/sil-project/SymfonySilMediaBundle",
    "authors": [
        {
            "name": "Romain SANCHEZ",
            "email": "romain.sanchez@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sil\\Bundle\\MediaBundle\\": "."
        }
    },
    "version": "dev-wip-platform",
    "config": {
        "bin-dir": "bin/"
    }
}
