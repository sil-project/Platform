{
    "name": "libre-informatique/ecommerce-bundle",
    "type": "symfony-bundle",
    "description": "Bridge bundle between sil bundles and sylius",
    "require": {
        "php": "^7.1",
        "symfony/symfony": "3.3.*",
        "blast-project/core-bundle": "self.version",
        "blast-project/base-entities-bundle": "self.version",
        "blast-project/search-bundle": "self.version",
        "blast-project/resource": "self.version",
        "blast-project/resource-bundle": "self.version",
        "blast-project/utils-bundle": "self.version",
        "blast-project/outer-extension-bundle": "self.version",
        "sylius/sylius": "^1.0",
        "libre-informatique/sonata-sylius-user-bundle": "self.version",
        "libre-informatique/crm-bundle": "self.version",
        "knplabs/knp-snappy-bundle": "^1.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "h4cc/wkhtmltoimage-amd64": "^0.12.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "3.3.*",
        "phpunit/phpunit": "^6.4"
    },
    "license": "LGPL-3.0-only",
    "keywords": [
        "e-commerce",
        "libio",
        "sonata",
        "symfony"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonySilEcommerceBundle",
    "authors": [
        {
            "name": "Marcos Bezerra de Menezes",
            "email": "marcos.bezerra@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sil\\Bundle\\EcommerceBundle\\": "."
        }
    },
    "version": "dev-wip-platform",
    "config": {
        "bin-dir": "bin/"
    }
}
