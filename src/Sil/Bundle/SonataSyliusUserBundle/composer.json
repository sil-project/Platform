{
    "name": "libre-informatique/sonata-sylius-user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle providing a bridge between SonataAdmin and SyliusUser (an alternative to SonataUserBundle)",
    "require": {
        "php": "^7.1",
        "symfony/symfony": "3.3.*",
        "sylius/sylius": "^1.0",
        "blast-project/core-bundle": "self.version",
        "blast-project/base-entities-bundle": "self.version"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "3.3.*",
        "blast-project/tests-bundle": "self.version",
        "phpunit/phpunit": "^6.4"
    },
    "license": "LGPL-3.0",
    "keywords": [
        "symfony",
        "sonata admin",
        "sylius"
    ],
    "homepage": "https://github.com/libre-informatique/SonataSyliusUserBundle",
    "authors": [
        {
            "name": "Marcos Bezerra de Menezes",
            "email": "marcos.bezerra@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sil\\Bundle\\SonataSyliusUserBundle\\": "."
        }
    },
    "version": "dev-wip-platform",
    "config": {
        "bin-dir": "bin/"
    }
}
