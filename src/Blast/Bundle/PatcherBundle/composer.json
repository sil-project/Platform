{
    "name": "blast-project/patcher-bundle",
    "type": "symfony-bundle",
    "description": "Patch Vendor",
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.4",
        "sensio/distribution-bundle": "^5.0",
        "sonata-project/doctrine-orm-admin-bundle": ">=3.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4",
        "blast-project/tests-bundle": "self.version",
        "symfony/console": ">=3.2",
        "phpunit/phpunit": "^6.4"
    },
    "scripts": {
        "test": [
    	    "phpunit"
    	],
        "test-full": [
    	    "@composer install"
