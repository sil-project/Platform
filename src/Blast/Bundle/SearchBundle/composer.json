    "description": "A search system coupled with ElasticSearch",
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.4",
        "sonata-project/block-bundle": "^3.9",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "knplabs/knp-paginator-bundle": "^2.7"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4",
        "phpunit/phpunit": "^6.4"
