    "keywords": [
        "blast",
        "sonata",
        "symfony",
        "search"
    ],
    "homepage": "https://github.com/blast-project/SearchBundle",
    "authors": [
        {
            "name": "Vivien FRASCA",
            "email": "vivien.frasca@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Blast\\Bundle\\DashboardBundle\\": "."
        }
    },
    "version": "dev-wip-platform",
    "config": {
        "bin-dir": "bin"
    }
